    "type": "Workflow"
  },
  "inputs": [
    {
      "name": "cidrBlock",
      "type": "string",
      "required": "yes",
      "description": "The IPv4 network range for the VPC, in CIDR notation",
      "exampleValue": "10.0.0.0/24"
    },
    {
      "name": "vpcName",
      "type": "string",
      "required": "yes",
      "description": "Name of the VPC",
      "exampleValue": "TEST VPC"
    },
    {
      "name": "adapterId",
      "type": "string",
      "required": "yes",
      "description": "AWS EC2 adapter to use",
      "exampleValue": "EC2"
    }
  ],
  "outputs": [
    {
